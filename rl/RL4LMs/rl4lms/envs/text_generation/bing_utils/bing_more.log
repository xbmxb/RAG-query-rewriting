                // Variables for script config
                var articleId = "23074";
                var adUnits = [];
                if (!true) {
                    adUnits = ["cchl", ""];
                }
                else {
                    adUnits = ["cchl", "cardio", "general", ""].filter(function (adCategory) { return adCategory; });
                }

                var adUnitPath = adUnits.join("/");
                var aimActive = true;
                var userIsInEU = false;

                // Set devices based on window width
                var windowWidth = window.innerWidth;
                var deviceType = (windowWidth >= 1200) ? "desktop" : "mobile";
                var mobileType = undefined;

                if (deviceType === "mobile") {
                    mobileType = (windowWidth >= 600) ? "tab" : "sph";
                }

                // Determine if user is in the EU
                var userTimeZone = Intl.DateTimeFormat().resolvedOptions().timeZone;
                userIsInEU = checkEUTimezone(userTimeZone);

                if (userIsInEU) {
                    aimActive = false;
                }

                window.TCAP = window.TCAP || {};

                // EDH Ad script config
                window.TCAP.adConfig = {
                    contentType: 'article',
                    adUnit: adUnitPath,
                    deviceType: deviceType,
                    contentTypeSlots: {
                        desktop: {
                            presentSlots: [
                                'billboard_300x250',
                                'billboard2_300x250',
                                'billboard3_300x250',
                                'billboard4_300x250',
                                'billboard5_300x250',
                                'leaderboard_728x90',
                                'leaderboard2_728x90'
                            ]
                        },
                        mobile: {
                            presentSlots: [
                                'adhesive_320x50_mobile',
                                'billboard_300x250_mobile',
                                'billboard2_300x250_mobile',
                                'billboard3_300x250_mobile'
                            ]
                        }
                    },
                    customParams: {
                        mdv: mobileType,
                        ugc: '0',
                        page: articleId
                    },
                    aim_active: aimActive
                }

                // Compares EU timezones with the timezone provided and returns a boolean
                function checkEUTimezone(timezone) {
                    switch (timezone) {
                        case 'Europe/Vienna':
                        case 'Europe/Brussels':
                        case 'Europe/Sofia':
                        case 'Europe/Zagreb':
                        case 'Asia/Famagusta':
                        case 'Asia/Nicosia':
                        case 'Europe/Prague':
                        case 'Europe/Copenhagen':
                        case 'Europe/Tallinn':
                        case 'Europe/Helsinki':
                        case 'Europe/Paris':
                        case 'Europe/Berlin':
                        case 'Europe/Busingen':
                        case 'Europe/Athens':
                        case 'Europe/Budapest':
                        case 'Europe/Dublin':
                        case 'Europe/Rome':
                        case 'Europe/Riga':
                        case 'Europe/Vilnius':
                        case 'Europe/Luxembourg':
                        case 'Europe/Malta':
                        case 'Europe/Amsterdam':
                        case 'Europe/Warsaw':
                        case 'Atlantic/Azores':
                        case 'Atlantic/Madeira':
                        case 'Europe/Lisbon':
                        case 'Europe/Bucharest':
                        case 'Europe/Bratislava':
                        case 'Europe/Ljubljana':
                        case 'Africa/Ceuta':
                        case 'Atlantic/Canary':
                        case 'Europe/Madrid':
                        case 'Europe/Stockholm':
                        case 'Europe/London':
                        case 'Europe/Belfast':
                            return true;
                        default:
                            return false;
                    }
                }
